{
    "name": "hnhdigital-os/laravel-multisite-router",
    "description": "Provides routing for multi-site applications",
    "keywords": ["laravel","router"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": ">=7.0",
        "hnhdigital-os/laravel-frontend-asset-loader": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "illuminate/database": "4.*|5.*",
        "symfony/process": "~2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\LaravelMultisiteRouter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\LaravelMultisiteRouter\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability" : "dev"
}
